
@{
    ViewData["Title"] = "Result";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="content">
    <div class="row m-t-3">
        <div class="col-lg-12">
            <div class="card ">
                <div class="card-header bg-blue">
                    <h5 class="text-white m-b-0">User Profile</h5>
                </div>
                <div class="card-body">

                    <form method="post">
                        <input type="hidden" id="ResultID" />
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group has-feedback">
                                    <label class="control-label">Test</label>
                                    <div class="Test"></div>
                                    <span class="fa fa-book form-control-feedback" aria-hidden="true"></span>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group has-feedback">
                                    <label class="control-label">Result Value</label>
                                    <input class="form-control" placeholder="Result Value" type="text">
                                    <span class="fa fa-user form-control-feedback" aria-hidden="true"></span>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group has-feedback">
                                    <label class="control-label">Date</label>
                                    <input class="form-control" placeholder="Result Date" type="date">
                                    <span class="fa fa-calendar form-control-feedback" aria-hidden="true"></span>
                                </div>
                            </div>

                            <div class="col-md-12">
                                <button type="submit" onclick="AddResult();" class="btn btn-success">Submit</button>
                            </div>
                        </div>
                    </form>

                </div>
            </div>
        </div>
    </div>
</div>
<partial name="Result_Details" />