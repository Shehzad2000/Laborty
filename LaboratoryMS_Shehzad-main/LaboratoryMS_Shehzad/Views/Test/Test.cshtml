
@{
    ViewData["Title"] = "Test";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="content">
    <div class="row m-t-3">
        <div class="col-lg-12">
            <div class="card ">
                <div class="card-header bg-blue">
                    <h5 class="text-white m-b-0">Test</h5>
                </div>
                <div class="card-body">

                    <form method="post">
                        <input type="hidden" id="TestID" />
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group has-feedback">
                                    <label class="control-label">Test Name</label>
                                    <input class="form-control" placeholder="Test Name" type="text">
                                    <span class="fa fa-flask form-control-feedback" aria-hidden="true"></span>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group has-feedback">
                                    <label class="control-label">Category</label>
                                    <div class="Category">

                                    </div>
                                    <span class="fa fa-list-alt form-control-feedback" aria-hidden="true"></span>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group has-feedback">
                                    <label class="control-label">Unit</label>
                                    <div class="Unit"></div>
                                    <span class="fa fa-balance-scale form-control-feedback" aria-hidden="true"></span>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group has-feedback">
                                    <label class="control-label">Normal Range</label>
                                    <input class="form-control" placeholder="Range" type="text">
                                    <span class="fa fa-spinner form-control-feedback" aria-hidden="true"></span>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group has-feedback">
                                    <label class="control-label">Patient</label>
                                    <input class="form-control" placeholder="Patient" type="text">
                                    <span class="fa fa-user form-control-feedback" aria-hidden="true"></span>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group has-feedback">
                                    <label class="control-label">Doctor</label>
                                    <input class="form-control" placeholder="Doctor" type="text">
                                    <span class="fa fa-user form-control-feedback" aria-hidden="true"></span>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group has-feedback">
                                    <label class="control-label">Contact</label>
                                    <input class="form-control" placeholder="Contact" type="text">
                                    <span class="fa fa-phone form-control-feedback" aria-hidden="true"></span>
                                </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group has-feedback">
                                        <label class="control-label">Gender</label>
                                        <input type="radio" name="gender" value="Male" checked />
                                        <input type="radio" name="gender" value="Female" />
                                        <input type="radio" name="gender" value="Unspecified" />
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group has-feedback">
                                        <label class="control-label">TestDate</label>
                                        <input class="form-control" placeholder="TestDate" type="date">
                                        <span class="fa fa-calendar form-control-feedback" aria-hidden="true"></span>
                                    </div>

                                    <div class="col-md-12">
                                        <button type="submit" onclick="AddTest();" class="btn btn-success">Submit</button>
                                    </div>
                                </div>
                    </div>
                            
                                </form>

                </div>
            </div>
        </div>
    </div>
</div>
<partial name="Test_Details" />